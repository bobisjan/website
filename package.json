{
  "name": "website",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "description": "Source for personal website",
  "repository": {
    "type": "git",
    "url": "https://github.com/bobisjan/website.git"
  },
  "license": "MIT",
  "author": "Jan Bobisud",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "vite build && vite build --ssr && node ./fastboot/cli.js /",
    "format": "prettier . --cache --check",
    "format:fix": "prettier . --cache --write",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\" --prefixColors auto",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\" --prefixColors auto",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache --concurrency=auto",
    "lint:js:fix": "eslint . --cache --concurrency=auto --fix",
    "lint:hbs": "ember-template-lint .",
    "start": "vite",
    "test": "vite build --mode development && vite build --ssr --mode development && percy exec -- testem ci -f testem.config.js",
    "prepare": "husky"
  },
  "devDependencies": {
    "@babel/core": "7.28.6",
    "@babel/eslint-parser": "7.28.6",
    "@babel/plugin-transform-runtime": "7.28.5",
    "@ember/test-helpers": "5.4.1",
    "@embroider/core": "4.4.2",
    "@embroider/legacy-inspector-support": "0.1.3",
    "@embroider/macros": "1.19.6",
    "@embroider/router": "3.0.6",
    "@embroider/vite": "1.5.0",
    "@eslint/css": "0.14.1",
    "@eslint/js": "9.39.2",
    "@glimmer/component": "2.0.0",
    "@percy/cli": "1.31.8",
    "@percy/ember": "5.0.0",
    "@percy/sdk-utils": "1.31.8",
    "@rollup/plugin-babel": "6.1.0",
    "babel-plugin-ember-template-compilation": "4.0.0",
    "concurrently": "9.2.1",
    "decorator-transforms": "2.3.1",
    "ember-modifier": "4.2.2",
    "ember-qunit": "9.0.4",
    "ember-source": "6.10.0",
    "ember-strict-application-resolver": "0.1.1",
    "ember-template-lint": "7.9.3",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-ember": "12.7.5",
    "eslint-plugin-n": "17.23.2",
    "eslint-plugin-qunit": "8.2.5",
    "globals": "17.1.0",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "prettier": "3.8.1",
    "prettier-plugin-ember-template-tag": "2.1.2",
    "qunit": "2.25.0",
    "qunit-dom": "3.5.0",
    "simple-dom": "1.4.0",
    "testem": "3.17.0",
    "vite": "7.3.1"
  },
  "imports": {
    "#src/*.js": "./src/*.js",
    "#src/*.gjs": "./src/*.gjs",
    "#tests/*.js": "./tests/*.js"
  },
  "ember": {
    "edition": "octane"
  },
  "engines": {
    "node": "24.12.0",
    "pnpm": "10.26.0"
  },
  "packageManager": "pnpm@10.26.0",
  "volta": {
    "node": "24.12.0",
    "pnpm": "10.26.0"
  },
  "ember-addon": {
    "type": "app",
    "version": 2
  }
}
