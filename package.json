{
  "name": "website",
  "version": "0.0.0",
  "private": true,
  "description": "Source for personal website",
  "repository": {
    "type": "git",
    "url": "https://github.com/bobisjan/website.git"
  },
  "license": "MIT",
  "author": "Jan Bobisud",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\" --prefixColors auto",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "concurrently \"pnpm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\" --prefixColors auto",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:format": "prettier . --check",
    "lint:format:fix": "prettier . --write",
    "lint:types": "glint",
    "start": "PREMBER=true ember serve",
    "test": "concurrently \"pnpm:lint\" \"pnpm:test:*\" --names \"lint,test:\" --prefixColors auto",
    "test:ember": "percy exec -- ember test",
    "prepare": "husky",
    "deploy": "ember deploy production"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@babel/eslint-parser": "^7.26.8",
    "@babel/plugin-proposal-decorators": "^7.25.9",
    "@ember/optional-features": "^2.2.0",
    "@ember/test-helpers": "^5.1.0",
    "@embroider/compat": "^3.8.3",
    "@embroider/core": "^3.5.2",
    "@embroider/webpack": "^4.1.0",
    "@eslint/js": "^9.21.0",
    "@glimmer/component": "^2.0.0",
    "@glimmer/tracking": "^1.1.2",
    "@glint/core": "^1.5.2",
    "@glint/environment-ember-loose": "^1.5.2",
    "@glint/environment-ember-template-imports": "^1.5.2",
    "@glint/template": "^1.5.2",
    "@percy/cli": "^1.30.7",
    "@percy/ember": "^4.2.0",
    "@tsconfig/ember": "^3.0.9",
    "@types/eslint__js": "^9.14.0",
    "@types/qunit": "^2.19.12",
    "@types/rsvp": "^4.0.9",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^9.1.2",
    "ember-auto-import": "^2.10.0",
    "ember-cli": "~6.2.2",
    "ember-cli-app-version": "^7.0.0",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-dependency-checker": "^3.3.3",
    "ember-cli-deploy": "^2.0.0",
    "ember-cli-deploy-build": "^3.0.0",
    "ember-cli-deploy-gzip": "^3.0.0",
    "ember-cli-deploy-manifest": "^3.0.0",
    "ember-cli-deploy-s3": "^5.0.1",
    "ember-cli-fastboot": "^4.1.5",
    "ember-cli-fastboot-testing": "^0.6.2",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-load-initializers": "^3.0.1",
    "ember-modifier": "^4.2.0",
    "ember-page-title": "^9.0.1",
    "ember-qunit": "^9.0.1",
    "ember-resolver": "^13.1.0",
    "ember-route-template": "^1.0.3",
    "ember-source": "~6.2.0",
    "ember-template-imports": "^4.3.0",
    "ember-template-lint": "^7.0.1",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-ember": "^12.5.0",
    "eslint-plugin-n": "^17.16.2",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-qunit": "^8.1.2",
    "globals": "^16.0.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.4.3",
    "loader.js": "^4.7.0",
    "prember": "^2.1.0",
    "prettier": "^3.5.3",
    "prettier-plugin-ember-template-tag": "^2.0.4",
    "qunit": "^2.24.1",
    "qunit-dom": "^3.4.0",
    "stylelint": "^16.15.0",
    "stylelint-config-standard": "^37.0.0",
    "tracked-built-ins": "^4.0.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.0",
    "webpack": "^5.98.0"
  },
  "overrides": {
    "@glimmer/tracking": {
      "@glimmer/validator": "0.84.3"
    }
  },
  "ember": {
    "edition": "octane"
  },
  "engines": {
    "node": "22.14.0",
    "pnpm": "10.4.1"
  },
  "packageManager": "pnpm@10.4.1",
  "volta": {
    "node": "22.14.0",
    "pnpm": "10.4.1"
  }
}
