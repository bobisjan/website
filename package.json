{
  "name": "website",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "description": "Source for personal website",
  "repository": {
    "type": "git",
    "url": "https://github.com/bobisjan/website.git"
  },
  "license": "MIT",
  "author": "Jan Bobisud",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "vite build && vite build --ssr && node ./fastboot/cli.js /",
    "format": "prettier . --cache --check",
    "format:fix": "prettier . --cache --write",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\" --prefixColors auto",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\" --prefixColors auto",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache --concurrency=auto",
    "lint:js:fix": "eslint . --cache --concurrency=auto --fix",
    "lint:hbs": "ember-template-lint .",
    "start": "vite",
    "test": "vite build --mode development && vite build --ssr --mode development && percy exec -- testem ci -f testem.config.js",
    "prepare": "husky"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/eslint-parser": "^7.28.4",
    "@babel/plugin-transform-runtime": "^7.28.3",
    "@ember/test-helpers": "^5.3.0",
    "@embroider/core": "^4.2.1",
    "@embroider/macros": "^1.18.2",
    "@embroider/vite": "^1.2.1",
    "@eslint/css": "^0.11.1",
    "@eslint/js": "^9.36.0",
    "@glimmer/component": "^2.0.0",
    "@percy/cli": "^1.31.2",
    "@percy/ember": "^5.0.0",
    "@percy/sdk-utils": "^1.31.2",
    "@rollup/plugin-babel": "^6.0.4",
    "babel-plugin-ember-template-compilation": "^3.0.1",
    "concurrently": "^9.2.1",
    "decorator-transforms": "^2.3.0",
    "ember-modifier": "^4.2.2",
    "ember-qunit": "^9.0.4",
    "ember-source": "~6.7.0",
    "ember-strict-application-resolver": "^0.1.0",
    "ember-template-lint": "^7.9.3",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-ember": "^12.7.4",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-qunit": "^8.2.5",
    "globals": "^16.4.0",
    "husky": "^9.1.6",
    "lint-staged": "^16.2.3",
    "prettier": "^3.6.2",
    "prettier-plugin-ember-template-tag": "^2.1.0",
    "qunit": "^2.24.1",
    "qunit-dom": "^3.5.0",
    "simple-dom": "^1.4.0",
    "testem": "^3.16.0",
    "vite": "^7.1.7"
  },
  "imports": {
    "#src/*.js": "./src/*.js",
    "#src/*.gjs": "./src/*.gjs",
    "#tests/*.js": "./tests/*.js"
  },
  "ember": {
    "edition": "octane"
  },
  "engines": {
    "node": "22.19.0",
    "pnpm": "10.15.1"
  },
  "packageManager": "pnpm@10.15.1",
  "volta": {
    "node": "22.19.0",
    "pnpm": "10.15.1"
  },
  "ember-addon": {
    "type": "app",
    "version": 2
  }
}
